{
    "rust-analyzer.updates.checkOnStartup": false,
    "rust-analyzer.procMacro.attributes.enable": true,
    "rust-analyzer.diagnostics.disabled": [
        "inactive-code"
    ],
    "coc.preferences.formatOnSaveFiletypes": [
        "rust"
    ],
    "suggest.noselect": false,
    "markdownlint.config": {
        "line-length": {
            "tables": false,
            "line_length": 100
        }
    },
    "languageserver": {
        "svls": {
            "command": "svls",
            "filetypes": [
                "systemverilog"
            ]
        }
    }
}
